openapi: 3.0.0
info:
  version: demo
  title: {{ProjectDesc}}
x-project-name: {{ProjectName}}
x-api-gateway-name: {{ProjectName}}RestApi
x-deployment-name: {{DeploymentName}}
x-deployment-region: {{DeploymentRegion}}
x-deployment-profile: {{DeploymentProfile}}
{{#AuthorizationId}}
x-api-authorizer-id: {{AuthorizationId}}
{{/AuthorizationId}}
paths:
  {{#ResourcePaths}}
  '/{{ResourcePath}}':
    {{#ResourceData}}
    {{#-first}}
    {{ResourceType}}-service-name: {{ServiceName}}
    {{#ServicePublic}}
    {{ResourceType}}-service-public: {{ServicePublic}}
    {{/ServicePublic}}
    {{#ServiceTemplate}}
    {{ResourceType}}-service-template: {{ServiceTemplate}}
    {{/ServiceTemplate}}
    {{#ServiceRuntime}}
    {{ResourceType}}-service-runtime: {{ServiceRuntime}}
    {{/ServiceRuntime}}
    {{#ServiceSchedule}}
    {{ResourceType}}-service-schedule: {{ServiceSchedule}}
    {{/ServiceSchedule}}
    {{#ServiceModel}}
    {{ResourceType}}-service-model-name: {{ServiceModel}}
    {{ResourceType}}-service-validation: false
    {{/ServiceModel}}
    {{#ServicePassthrough}}
    {{ResourceType}}-service-passthrough: {{ServicePassthrough}}
    {{/ServicePassthrough}}
    {{/-first}}
    {{ResourceMethod}}:
      {{#ServiceTag}}
      tags:
        - {{ServiceTag}}
      {{/ServiceTag}}
      description: {{Description}} Information
      operationId: {{OperationName}}
      responses:
        '200':
          description: Success
    {{/ResourceData}}
  {{/ResourcePaths}}
servers:
  - url: /